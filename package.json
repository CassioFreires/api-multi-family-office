{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "migrate:dev": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "rollback:dev": "npx prisma migrate reset",
    "compile:seed": "tsc --outDir ./dist-seed --esModuleInterop --module NodeNext prisma/seed.ts",
    "run:seed": "node ./dist-seed/seed.js",
    "db:seed": "npm run compile:seed && npm run generate && npm run run:seed",
    "clean:seed": "rm -rf ./dist-seed",
     "test": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "fastify": "^5.6.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5"
  }
}